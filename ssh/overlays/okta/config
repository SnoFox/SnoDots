  UserKnownHostsFile ~/.local/share/ScaleFT/proxycommand_known_host

Host *.aue1s.internal *.aec1s.internal
  ProxyCommand "/usr/bin/sft" proxycommand --team okta-preview %h
  UserKnownHostsFile ~/.local/share/ScaleFT/proxycommand_known_host

